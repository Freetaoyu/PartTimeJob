<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
     >
    <TextView
        android:id="@+id/gongsi_head_1_textview01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="定海皇家酒店"
        android:layout_marginTop="18dp"
        android:layout_marginLeft="16dp"
        android:textColor="#000000"
        />
    <ImageView
        android:id="@+id/gongsi_head_1_imageview01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="11dp"
        android:layout_below="@+id/gongsi_head_1_textview01"
        android:layout_marginLeft="16dp"
        android:src="@drawable/gsxq_icon_qyrz_nor"/>

    <ImageView
        android:id="@+id/gongsi_head_1_imageview02"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/gsxq_icon_bzj_nor"
        android:layout_marginTop="11dp"
        android:layout_below="@+id/gongsi_head_1_textview01"
        android:layout_toRightOf="@+id/gongsi_head_1_imageview01"
        android:layout_marginLeft="16dp"
        />
    <TextView
        android:layout_width="160dp"
        android:layout_height="wrap_content"
        android:text="公司简介+我们位于浙江舟山定海中大街21号"
        android:layout_below="@+id/gongsi_head_1_imageview02"
        android:layout_marginTop="9dp"
        android:textSize="11sp"
        android:layout_alignLeft="@+id/gongsi_head_1_textview01"
        android:textColor="#888888"/>
    
    <ImageView
        android:id="@+id/gongsi_head_1_imageview_01"
        android:layout_marginTop="52dp"
        android:layout_alignParentRight="true"
        android:layout_marginRight="60dp"
        android:layout_width="37dp"
        android:layout_height="37dp"
        android:src="@drawable/gsxq_icon_photo_nor"
        />
    <ImageView
        android:id="@+id/gongsi_head_1_imageview_02"
        android:layout_marginTop="52dp"
        android:layout_alignParentRight="true"
        android:layout_marginRight="16dp"
        android:layout_width="37dp"
        android:layout_height="37dp"
        android:src="@drawable/gsxq_icon_photo_nor"
        />
    <ImageView
        android:id="@+id/gongsi_head_1_imageview_03"
        android:layout_marginTop="96dp"
        android:layout_alignParentRight="true"
        android:layout_marginRight="60dp"
        android:layout_width="37dp"
        android:layout_height="37dp"
        android:src="@drawable/gsxq_icon_photo_nor"
        />
    <ImageButton
        android:id="@+id/gongsi_head_1_imageview_add"
         android:layout_marginTop="96dp"
        android:layout_alignParentRight="true"
        android:layout_marginRight="16dp"
        android:layout_width="37dp"
        android:layout_height="37dp"
        android:src="@drawable/gsxq_icon_photomor_nor"
        android:background="@null"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="7dp"
        android:layout_marginTop="12dp"
        android:layout_below="@+id/gongsi_head_1_imageview_add"
        android:background="#ececec"/>
</RelativeLayout>
