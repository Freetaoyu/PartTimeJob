<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff" >

    <RelativeLayout
        android:id="@+id/rw_layout_rey01"
        android:layout_width="match_parent"
        android:layout_height="49dp"
        android:background="#ffffff"
        android:gravity="center_vertical" >

        <ImageView
            android:id="@+id/about_us_return_imageview"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:src="@drawable/myjzxq_icon_back_nor" />

        <ImageButton
            android:id="@+id/mycreadit_return_button"
            android:layout_width="60dp"
            android:layout_height="30dp"
            android:background="@null" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="任务详情"
            android:textSize="18sp" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/wait_fresh_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/partjob_bottom_rey09"
        android:layout_below="@+id/rw_layout_rey01" >

        <ImageView
            android:id="@+id/parallax_imageview"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:src="@drawable/ic_launcher" />
    </RelativeLayout>

    <ScrollView
        android:id="@+id/parallax_scroll_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/rw_layout_rey01"
        android:orientation="vertical"
        android:overScrollMode="never"
        android:visibility="gone" >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <LinearLayout
                android:id="@+id/job_linear01"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/headview"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:scaleType="centerCrop"
                    android:src="@drawable/jianzhi1" />
            </LinearLayout>

            <TextView
                android:id="@+id/text_xian"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/job_linear01" />

            <ImageView
                android:id="@+id/job_textview01"
                android:layout_width="72dp"
                android:layout_height="72dp"
                android:layout_alignBottom="@+id/job_textview03"
                android:layout_marginLeft="21dp"
                android:scaleType="centerInside"
                android:src="@drawable/jianzhi1" />

            <TextView
                android:id="@+id/job_textview02"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/job_linear01"
                android:layout_marginLeft="11dp"
                android:layout_marginTop="6dp"
                android:layout_toRightOf="@+id/job_textview01"
                android:text="招聘淘宝头条写手"
                android:textColor="#333333" />

            <TextView
                android:id="@+id/job_textview03"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/job_textview02"
                android:layout_below="@+id/job_textview02"
                android:layout_marginTop="10dp"
                android:text="实习生"
                android:textColor="#6D6D6D"
                android:textSize="10sp" />

            <TextView
                android:id="@+id/job_textview_month"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/job_textview02"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:layout_toRightOf="@+id/job_textview03"
                android:text="月结"
                android:textColor="#68bd61"
                android:textSize="10sp" />

            <TextView
                android:id="@+id/job_textview04"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/job_textview03"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@+id/job_textview_month"
                android:text="7元/时"
                android:textSize="10sp" />

            <TextView
                android:id="@+id/job_textview05"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_below="@+id/job_textview02"
                android:layout_marginRight="60dp"
                android:text="截止日期："
                android:textColor="#6d6d6d"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/job_textview06"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/job_textview05"
                android:layout_below="@+id/job_textview05"
                android:layout_marginTop="6dp"
                android:text="2016年12月4日"
                android:textColor="#6d6d6d"
                android:textSize="15sp" />

            <RelativeLayout
                android:id="@+id/partjob_rey01"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/job_textview06"
                android:layout_marginTop="30dp" >

                <ImageView
                    android:id="@+id/partjob_imageview01"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="21dp"
                    android:layout_marginTop="3dp"
                    android:src="@drawable/defult_icon_logo_nor" />

                <TextView
                    android:id="@+id/partjob_textview07"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="8dp"
                    android:layout_toRightOf="@+id/partjob_imageview01"
                    android:text="工作日期:"
                    android:textSize="10sp" />

                <TextView
                    android:id="@+id/textview_date"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="6dp"
                    android:layout_toRightOf="@+id/partjob_textview07"
                    android:text="2016年-11-27至2017年12-27"
                    android:textColor="#333333"
                    android:textSize="10sp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/partjob_rey02"
                android:layout_width="match_parent"
                android:layout_height="27dp"
                android:layout_below="@+id/partjob_rey01"
                android:layout_marginTop="9dp" >

                <ImageView
                    android:id="@+id/partjob_imageview02"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="21dp"
                    android:layout_marginTop="3dp"
                    android:src="@drawable/defult_icon_location_nor" />

                <TextView
                    android:id="@+id/partjob_textview08"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="8dp"
                    android:layout_toRightOf="@+id/partjob_imageview02"
                    android:text="工作地点:"
                    android:textColor="#333333"
                    android:textSize="10sp" />

                <TextView
                    android:id="@+id/partjob_textview09"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="6dp"
                    android:layout_toRightOf="@+id/partjob_textview08"
                    android:text="浙江省舟山市定海区中大街21号"
                    android:textColor="#333333"
                    android:textSize="10sp" />

                <Button
                    android:layout_width="wrap_content"
                    android:layout_height="13dp"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="16dp"
                    android:background="@null"
                    android:text="查看路线"
                    android:textColor="#448aca"
                    android:textSize="10sp" />
            </RelativeLayout>

            <TextView
                android:id="@+id/partjob_textview10"
                android:layout_width="match_parent"
                android:layout_height="6dp"
                android:layout_below="@+id/partjob_rey02"
                android:background="#ececec" />

            <RelativeLayout
                android:id="@+id/partjob_rey03"
                android:layout_width="match_parent"
                android:layout_height="36dp"
                android:layout_below="@+id/partjob_textview10"
                android:background="#ffffff" >

                <Button
                    android:id="@+id/jzxq_button_togongsi"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@null" />

                <ImageView
                    android:id="@+id/partjob_imageview03"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="8dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="8dp"
                    android:src="@drawable/parttime_juxing" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="8dp"
                    android:layout_marginLeft="9dp"
                    android:layout_marginTop="8dp"
                    android:layout_toRightOf="@+id/partjob_imageview03"
                    android:text="公司详情"
                    android:textColor="#448aca" />

                <ImageButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="16dp"
                    android:background="@null"
                    android:src="@drawable/myjzxx_icon_delmore_nor" />
            </RelativeLayout>

            <TextView
                android:id="@+id/partjob_textview11"
                android:layout_width="match_parent"
                android:layout_height="6dp"
                android:layout_below="@+id/partjob_rey03"
                android:background="#ececec" />

            <RelativeLayout
                android:id="@+id/partjob_rey04"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/partjob_textview11" >

                <ImageView
                    android:id="@+id/partjob_imageview04"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="8dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="8dp"
                    android:src="@drawable/parttime_juxing" />

                <TextView
                    android:id="@+id/partjob_textview12"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="8dp"
                    android:layout_marginLeft="9dp"
                    android:layout_marginTop="8dp"
                    android:layout_toRightOf="@+id/partjob_imageview04"
                    android:text="工作简介"
                    android:textColor="#448aca" />

                <TextView
                    android:id="@+id/partjob_textview13"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/partjob_textview12"
                    android:layout_marginLeft="42dp"
                    android:layout_marginRight="30dp"
                    android:layout_marginTop="3dp"
                    android:lineSpacingMultiplier="1.2"
                    android:text="我们是正规的淘宝店铺,平时的工作室根据提供的图片和  文案设计相关的banner，头条文案宣传.优秀兼职课兼职可录成为长期兼职生.希望来参与工作的人能保持热情。"
                    android:textColor="#333333" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="12dp"
                    android:layout_below="@+id/partjob_textview13" />
            </RelativeLayout>

            <TextView
                android:id="@+id/partjob_textview14"
                android:layout_width="match_parent"
                android:layout_height="6dp"
                android:layout_below="@+id/partjob_rey04"
                android:background="#ececec" />

            <RelativeLayout
                android:id="@+id/partjob_rey05"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/partjob_textview14" >

                <ImageView
                    android:id="@+id/partjob_imageview05"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="8dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="8dp"
                    android:src="@drawable/parttime_juxing" />

                <TextView
                    android:id="@+id/partjob_textview15"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="8dp"
                    android:layout_marginLeft="9dp"
                    android:layout_marginTop="8dp"
                    android:layout_toRightOf="@+id/partjob_imageview05"
                    android:text="基本要求"
                    android:textColor="#448aca" />

                <TextView
                    android:id="@+id/partjob_textview16"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/partjob_textview15"
                    android:layout_marginLeft="42dp"
                    android:layout_marginRight="30dp"
                    android:layout_marginTop="12dp"
                    android:text="1.芝麻信用分必须大于650分半\n2.熟悉淘宝购物流程，有丰富的购物经验.\n3.有较好的编程能力和审美能力.\n4.会AI.PS。等软件\n5.每天工作6到8小时。" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="14dp"
                    android:layout_below="@+id/partjob_textview16" />
            </RelativeLayout>

            <TextView
                android:id="@+id/partjob_textview17"
                android:layout_width="match_parent"
                android:layout_height="6dp"
                android:layout_below="@+id/partjob_rey05"
                android:background="#ececec" />

            <ListView
                android:id="@+id/partjob_listview"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/partjob_textview17" />

            <RelativeLayout
                android:id="@+id/partjob_rey08"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_below="@+id/partjob_listview"
                android:background="#ececec"
                android:gravity="center" >

                <TextView
                    android:id="@+id/partjob_textview23"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="注：具体内容已商家为准"
                    android:textColor="#999999"
                    android:textSize="10sp" />

                <TextView
                    android:id="@+id/partjob_textview24"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/partjob_textview23"
                    android:layout_marginTop="4dp"
                    android:text="报名后请及时联系"
                    android:textColor="#999999"
                    android:textSize="10sp" />

                <Button
                    android:layout_width="wrap_content"
                    android:layout_height="15dp"
                    android:layout_below="@+id/partjob_textview23"
                    android:layout_marginTop="3dp"
                    android:layout_toRightOf="@+id/partjob_textview24"
                    android:background="@null"
                    android:text="联系商家"
                    android:textColor="#448aca"
                    android:textSize="10sp" />
            </RelativeLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="88dp"
                android:layout_below="@+id/partjob_rey08"
                android:background="@color/white" />
        </RelativeLayout>
    </ScrollView>

    <RelativeLayout
        android:id="@+id/partjob_bottom_rey09"
        android:layout_width="match_parent"
        android:layout_height="83dp"
        android:visibility="gone"
        android:layout_alignParentBottom="true" >

        <RelativeLayout
            android:id="@+id/partjob_rey09"
            android:layout_width="match_parent"
            android:layout_height="28dp"
            android:background="#ffffff" >

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="18dp" >

                <TextView
                    android:id="@+id/partjob_textview25"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="总需人数：" />

                <TextView
                    android:id="@+id/partjob_textview26"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_toRightOf="@+id/partjob_textview25"
                    android:text="100人" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="18dp" >

                <TextView
                    android:id="@+id/partjob_textview27"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="已报名：" />

                <TextView
                    android:id="@+id/partjob_textview28"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_toRightOf="@+id/partjob_textview27"
                    android:text="50人" />
            </RelativeLayout>
        </RelativeLayout>

        <ImageButton
            android:id="@+id/jzxq_baoming_button"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:layout_marginLeft="25dp"
            android:layout_marginRight="25dp"
            android:layout_below="@+id/partjob_rey09"
            android:background="@null"
            android:src="@drawable/myrwxq_bar_ljbm_nor" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="10dp"
            android:layout_below="@+id/jzxq_baoming_button"
            android:background="@color/white" />
    </RelativeLayout>

</RelativeLayout>