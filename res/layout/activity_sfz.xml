<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="0dp"
    android:background="#ffffff"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".Sfz_activity" >

    <ScrollView 
        android:id="@+id/mysfrz_scrollview"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <RelativeLayout
                android:id="@+id/mysfrz_rey01"
                android:layout_width="match_parent"
                android:layout_height="49dp"
                android:background="#ffffff"
                android:gravity="center_vertical" >

                <ImageView
                    android:id="@+id/about_us_return_imageview"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="25dp"
                    android:src="@drawable/mylogin_icon_back_nor" />

                <ImageButton
                    android:id="@+id/mycreadit_return_button"
                    android:layout_width="60dp"
                    android:layout_height="30dp"
                    android:background="@null" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:text="信息认证"
                    android:textSize="18sp" />
            </RelativeLayout> 
         <TextView
             android:id="@+id/mysfrz_textview_01"
             android:layout_below="@+id/mysfrz_imageview_01"
             android:layout_width="match_parent"
             android:layout_height="8dp"
             android:background="#ececec"
             />
         <RelativeLayout
             android:id="@+id/mysfrz_rey_02"
             android:layout_below="@+id/mysfrz_textview_01"
             android:layout_width="match_parent"
             android:layout_height="34dp">
                <ImageView
                    android:id="@+id/mysfrz_textview_02"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="16dp"
                    android:src="@drawable/myidcard_bar_mean_nor"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="14sp"
                    android:text="身份认证"
                    android:layout_toRightOf="@+id/mysfrz_textview_02"
                    android:layout_marginLeft="12dp"
                    android:layout_centerVertical="true"/>
          </RelativeLayout>
          <TextView
              android:id="@+id/mysfrz_textview_03"
              android:layout_below="@+id/mysfrz_rey_02"
              android:layout_width="match_parent"
              android:layout_height="1dp"
              android:background="#ececec"/>
          
          <RelativeLayout
              android:id="@+id/mysfrz_rey_03"
              android:layout_below="@+id/mysfrz_textview_03"
              android:layout_width="match_parent"
              android:layout_height="34dp"
              >
              <TextView
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:text="身份证"
                  android:layout_marginLeft="16dp"
                  android:textSize="14dp"
                  android:layout_centerVertical="true"
                  android:textColor="#888888"/>
              <Button
                  android:id="@+id/mysfrz_button_01"
                  android:layout_width="100dp"
                  android:layout_height="wrap_content"
                  android:text="立即认证"
                  android:textColor="#448aca"
                  android:background="@null"
                  android:textSize="14dp"
                  android:layout_alignParentRight="true"                  
                  android:layout_marginRight="16dp"
                  android:layout_centerVertical="true"/>
          </RelativeLayout>
          <TextView
              android:id="@+id/mysfrz_textview_04"
              android:layout_below="@+id/mysfrz_rey_03"
              android:layout_width="match_parent"
              android:layout_height="1dp"
              android:background="#ececec"/>
          <RelativeLayout
              android:id="@+id/mysfrz_rey_04"
              android:layout_below="@+id/mysfrz_textview_04"
              android:layout_width="match_parent"
              android:layout_height="34dp">
              <TextView
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:text="学生证"
                  android:layout_marginLeft="16dp"
                  android:textSize="14dp"
                  android:layout_centerVertical="true"
                  android:textColor="#888888"/>
              <Button
                  android:id="@+id/mysfrz_button_02"
                  android:layout_width="100dp"
                  android:layout_height="wrap_content"
                  android:text="立即认证"
                  android:textColor="#448aca"
                  android:background="@null"
                  android:textSize="14dp"
                  android:layout_alignParentRight="true"                  
                  android:layout_marginRight="16dp"
                  android:layout_centerVertical="true"/>
          </RelativeLayout>
          <TextView
              android:id="@+id/mysfrz_textview_05"
               android:layout_below="@+id/mysfrz_rey_04"
              android:layout_width="match_parent"
              android:layout_height="8dp"
              android:background="#ececec"/>
          
          <RelativeLayout
             android:id="@+id/mysfrz_rey_05"
             android:layout_below="@+id/mysfrz_textview_05"
             android:layout_width="match_parent"
             android:layout_height="34dp">
                <ImageView
                    android:id="@+id/mysfrz_textview_06"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="16dp"
                    android:src="@drawable/myidcard_bar_mean_nor"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="14sp"
                    android:text="已有认证"
                    android:layout_toRightOf="@+id/mysfrz_textview_06"
                    android:layout_marginLeft="12dp"
                    android:layout_centerVertical="true"/>
          </RelativeLayout>
          
          <TextView
              android:id="@+id/mysfrz_textview_07"
              android:layout_below="@+id/mysfrz_rey_05"
              android:layout_width="match_parent"
              android:layout_height="1dp"
              android:background="#ececec"/>
          
          <TextView
              android:id="@+id/mysfrz_textview_09"
               android:layout_below="@+id/mysfrz_textview_07"
              android:layout_width="match_parent"
              android:layout_height="8dp"
              android:background="#ececec"/>
          <RelativeLayout
             android:id="@+id/mysfrz_rey_08"
             android:layout_below="@+id/mysfrz_textview_09"
             android:layout_width="match_parent"
             android:layout_height="34dp">
                <ImageView
                    android:id="@+id/mysfrz_textview_10"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="16dp"
                    android:src="@drawable/myidcard_bar_mean_nor"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="14sp"
                    android:text="待审核"
                    android:layout_toRightOf="@+id/mysfrz_textview_10"
                    android:layout_marginLeft="12dp"
                    android:layout_centerVertical="true"/>
          </RelativeLayout>
           <TextView
              android:id="@+id/mysfrz_textview_11"
              android:layout_below="@+id/mysfrz_rey_08"
              android:layout_width="match_parent"
              android:layout_height="1dp"
              android:background="#ececec"/>
          
          
          <TextView
              android:id="@+id/mysfrz_textview_12"
              android:layout_below="@+id/mysfrz_rey_09"
              android:layout_width="match_parent"
              android:layout_height="1dp"
              android:background="#ececec"/>
          
          
          <ImageButton
              android:id="@+id/mysfrz_imagebutton_1"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:src="@drawable/selected_button_add"
              android:layout_centerHorizontal="true"
              android:layout_below="@+id/mysfrz_rey_08"
              android:layout_marginTop="30dp"
              android:background="@null"
              />
          <TextView
              android:layout_below="@+id/mysfrz_imagebutton_1"
              android:layout_width="match_parent"
              android:layout_height="30dp"/>

          <ImageView
              android:id="@+id/mysfrz_imageview_01"
              android:layout_width="fill_parent"
              android:layout_height="162dp"
              android:layout_alignParentLeft="true"
              android:scaleType="fitXY"
              android:layout_below="@+id/mysfrz_rey01"
              android:background="@drawable/sfrz_2" />

          </RelativeLayout>
          </ScrollView>
</RelativeLayout>
