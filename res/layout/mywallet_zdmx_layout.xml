<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/gray_1"
    >
    

    <RelativeLayout
        android:id="@+id/mymywallet_zdmx_rey01"
        android:layout_width="match_parent"
        android:layout_height="49dp"
        android:background="#ffffff" >

        <ImageView
            android:id="@+id/mymywallet_zdmx_return_imageview"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="25dp"
            android:src="@drawable/mylogin_icon_back_nor" />

        <ImageButton
            android:id="@+id/mymywallet_zdmx_return_button"
            android:layout_width="60dp"
            android:layout_height="match_parent"
            android:background="@null" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="账单明细"
            android:textSize="18sp" />
    </RelativeLayout>
    
    <ListView
        android:id="@+id/mymywallet_zdmx_listview"
        android:layout_marginTop="2dp"
        android:scrollbars="none"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/mymywallet_zdmx_rey01"
        />
</RelativeLayout>
