<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
     >
    

     <TextView
         android:id="@+id/text_head_item_my"
        android:layout_width="match_parent"
        android:layout_height="20dp"
        />
    <ImageView
        android:id="@+id/image_mylist_view_iteam"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/text_head_item_my"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:layout_marginLeft="20dp"
        android:src="@drawable/defult_icon_resume_nor"
        />
    <TextView
        android:id="@+id/text_mylist_contact"
        android:layout_toRightOf="@+id/image_mylist_view_iteam"
        android:layout_marginLeft="20dp"
        android:textSize="15sp"
        android:layout_below="@+id/text_head_item_my"
        android:textColor="@color/gray_8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:text="12121"
        />
    <ImageView
        android:layout_below="@+id/text_head_item_my"
        android:layout_width="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_height="wrap_content"
        android:layout_marginRight="15dp"
        android:src="@drawable/my_tab_go_nor"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:layout_below="@+id/text_mylist_contact"
        />
</RelativeLayout>
