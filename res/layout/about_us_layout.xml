<com.clv.parttimejobs.view.ui.customview.SildingFinishLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
     android:id="@+id/sildingFinishLayout"
    android:background="@color/gray_2"
    android:padding="0dp"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.clv.parttimejobs.activity.minelayout.aboutus.AboutUs" >

    <ScrollView 
        android:id="@+id/about_us_scrollview"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <RelativeLayout
        android:id="@+id/about_us_rey01"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:gravity="center_vertical"
        android:background="@color/white" >

        <ImageView
            android:id="@+id/about_us_return_imageview"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:src="@drawable/mylogin_icon_back_nor" />

         <ImageButton
            android:id="@+id/about_us_return_button"
            android:layout_width="69dp"
            android:layout_height="30dp"
            android:background="@null"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true" 
            android:textSize="18sp"
            android:text="联系客服"/>
    </RelativeLayout>

    <LinearLayout
        android:id="@+id/about_us_lin01"
        android:layout_width="match_parent"
        android:layout_height="350dp"
        android:layout_below="@+id/about_us_rey01"
        android:background="@color/gray_2"
        android:gravity="center" >

        <ImageView
            android:layout_width="220dp"
            android:layout_height="240dp"
            android:layout_below="@+id/about_us_return_imageview"
            android:layout_centerHorizontal="true"
            android:scaleType="fitXY"
            android:src="@drawable/conecttouspicture" />
    </LinearLayout>

    <RelativeLayout
        android:id="@+id/about_us_reylay01"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_below="@+id/about_us_lin01"
        android:layout_marginLeft="9dp"
        android:layout_marginRight="9dp"
        android:background="@color/white"
        android:gravity="center" >

        <TextView
            android:id="@+id/t1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="在线咨询"
            android:textSize="18sp" />

        <Button
            android:id="@+id/clickto"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="12dp"
            android:layout_toRightOf="@+id/t1"
            android:layout_alignTop="@+id/t1"
            android:background="@null"
            android:text="点击进入"
            android:textColor="@color/qianblue"
            android:textSize="18sp" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/about_us_reylay02"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_below="@+id/about_us_reylay01"
        android:layout_marginLeft="9dp"
        android:layout_marginRight="9dp"
        android:layout_marginTop="5dp"
        android:background="@color/white"
        android:gravity="center_vertical" 
        >

        <TextView
            android:id="@+id/t2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="92dp"
            android:text="咨询电话"
            android:textSize="18sp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="12dp"
            android:layout_toRightOf="@+id/t2"
            android:text="XXXXXXXXXXX"
            android:textColor="@color/qianblue"
            android:textSize="18sp" />
    </RelativeLayout>

    </RelativeLayout>
    </ScrollView>
</com.clv.parttimejobs.view.ui.customview.SildingFinishLayout>